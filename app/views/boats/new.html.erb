<%= simple_form_for @boat do |f| %>
  <%= f.error_notification %>

  <%= f.input :title %>

  <%= f.input :description %>

  <%= f.input :photo_url %>

  <%= f.input :price %>

  <%= f.input :category %>

  <%= f.input :address %>



  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>
